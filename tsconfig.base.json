{
  "compilerOptions": {
    "composite": true,
    "target": "ES2021",
    "lib": [
      "ES2021"
    ],
    "baseUrl": "./",
    "module": "Preserve",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,

    "noUnusedParameters": true,
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true
  }
}
