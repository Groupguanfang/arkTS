{
  "app": {
    "signingConfigs": [
      {
        "name": "autosign",
        "material": {
          "storePassword": "123456",
          "certpath": "./autosign/OpenHarmonyProfileRelease.pem",
          "keyAlias": "OpenHarmony Application Profile Release",
          "keyPassword": "123456",
          "profile": "./autosign/autosign.p7b",
          "signAlg": "SHA256withECDSA",
          "storeFile": "./autosign/OpenHarmony.p12"
        }
      }
    ],
    "products": [
      {
        "name": "default",
        "signingConfig": "autosign",
        "compileSdkVersion": "18",
        "compatibleSdkVersion": "18",
        "runtimeOS": "OpenHarmony",
        "targetSdkVersion": "18",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug"
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "sampleLibrary",
      "srcPath": "./sampleLibrary",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    }
  ]
}