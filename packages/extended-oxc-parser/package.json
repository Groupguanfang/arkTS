{
  "name": "@arkts/oxc-parser",
  "type": "module",
  "version": "0.0.1",
  "description": "Add arkts struct&#x27;s and function decorator support to OxC Parser.",
  "author": "Naily Zero <zero@naily.cc> (https://naily.cc)",
  "keywords": [
    "es-project-template"
  ],
  "exports": {
    "./index": {
