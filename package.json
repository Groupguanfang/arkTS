{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/volarjs/starter.git"
  },
  "scripts": {
    "build": "cd ./packages/vscode && npm run build",
    "watch": "cd ./packages/vscode && npm run watch",
    "pack": "cd ./packages/vscode && npm run pack",
    "generate": "cd ./packages/vscode && npm run update",
    "release": "pnpm -F \"{packages/*}\" build && tsx ./scripts/publish.ts",
    "test": "vitest",
    "lint": "eslint && oxlint && tsc --build --noEmit",
    "prepare": "pnpm -F \"{packages/*}\" build"
  },
  "dependencies": {
    "@types/progress-stream": "catalog:dev",
    "@vitejs/plugin-vue": "catalog:frontend",
    "@vitejs/plugin-vue-jsx": "catalog:frontend",
    "@vueuse/core": "catalog:frontend",
    "pinia": "catalog:frontend",
    "reflect-metadata": "catalog:polyfill",
    "unocss": "catalog:frontend",
    "unplugin-swc": "catalog:build",
    "vite": "catalog:frontend",
    "vue": "catalog:frontend"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@antfu/nip": "catalog:scripts",
    "@arkts/shared": "workspace:*",
    "@changesets/changelog-github": "catalog:scripts",
    "@changesets/cli": "catalog:scripts",
    "@release-it/bumper": "catalog:scripts",
    "@types/lodash": "catalog:prod",
    "@types/node": "catalog:dev",
    "@types/unzipper": "catalog:prod",
    "consola": "catalog:prod",
    "cross-env": "catalog:scripts",
    "defu": "catalog:prod",
    "eslint": "catalog:lint",
    "html5parser": "^2.0.2",
    "lodash": "catalog:prod",
    "magic-string": "^0.30.17",
    "oxc-parser": "catalog:scripts",
    "oxc-walker": "catalog:scripts",
    "oxlint": "catalog:lint",
    "pnpm": "catalog:scripts",
    "reactive-vscode": "catalog:dev",
    "simple-git-hooks": "catalog:scripts",
    "tsdown": "catalog:build",
    "tsx": "catalog:scripts",
    "type-fest": "catalog:dev",
    "typescript": "catalog:prod",
    "unzipper": "catalog:prod",
    "vscode-ext-gen": "catalog:dev",
    "vscode-languageserver-textdocument": "catalog:prod",
    "vscode-nls": "catalog:prod"
  }
}
